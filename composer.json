{
    "name": "atanvarno/middleware-dispatch",
    "description": "PSR-15 middleware dispatcher",
    "keywords": [
        "PSR-15", "middleware", "dispatcher"
    ],
    "homepage": "https://github.com/atanvarno69/middleware-dispatch",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-server-handler": "^1.0.0",
        "psr/http-server-middleware": "^1.0.0",
        "http-interop/http-factory": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.5",
        "http-interop/http-factory-diactoros": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Middleware\\Dispatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Middleware\\Dispatch\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
