{
    "name": "atanvarno/simple-middleware-dispatch",
    "description": "Basic PSR-15 middleware dispatcher",
    "keywords": [
        "PSR-15", "middleware", "dispatcher"
    ],
    "homepage": "https://github.com/atanvarno69/simple-middleware-dispatch",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-server-middleware": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.5",
        "atanvarno/middleware-util": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Middleware\\Dispatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Middleware\\Dispatch\\Test\\": "tests"
        }
    },
    "provide": {
        "psr/http-server-middleware-dispatcher-implementation": "1.0.0"
    },
    "suggest": {
        "atanvarno/middleware-util": "Provides a final handler"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
